            <textevent timeoffset="21" message="Again, focus on your cadence in each of these rest sections"/>
            <textevent timeoffset="32" message="You should be feeling nice and warm now"/>
            <textevent timeoffset="270" message="Ready for the first set?"/>
            <textevent timeoffset="281" message="It may aid you to get out the saddle in the 30 sec efforts as you would outside"/>
        </SteadyState>
